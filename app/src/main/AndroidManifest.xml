<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.connect">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:name=".AuthenticationActivities.WebSocketService"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NavigationBar">
        <activity android:name=".TeamActivity" />
        <activity android:name=".MainActivity" android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".AuthenticationActivities.UserValidationActivity"></activity>
        <activity android:name=".AuthenticationActivities.GettingStartedActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AuthenticationActivities.LoginActivity" />
        <activity android:name=".AuthenticationActivities.RegisterActivity" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>